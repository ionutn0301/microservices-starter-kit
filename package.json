{
  "name": "microservices-starter-kit",
  "version": "1.0.0",
  "description": "A comprehensive microservices starter kit with NestJS, PostgreSQL, RabbitMQ, and Redis",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "start": "turbo run start",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "prisma:generate": "turbo run prisma:generate",
    "docker:up": "docker-compose -f infra/docker-compose.yml up -d",
    "docker:down": "docker-compose -f infra/docker-compose.yml down",
    "docker:logs": "docker-compose -f infra/docker-compose.yml logs -f",
    "prisma:migrate": "turbo run prisma:migrate",
    "prisma:studio": "turbo run prisma:studio"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.0",
    "turbo": "^1.10.12",
    "typescript": "^5.1.6"
  },
  "packageManager": "npm@9.8.1",
  "dependencies": {
    "@fastify/static": "^7.0.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2"
  }
}
